                    h3.modal-title Meeting Info
                    button.ed-icon-close(type="button" class="close" data-target="#modalSessionInfo" data-dismiss="modal") x
                .modal-body
                    .ed-modal-meeting-info-wrapper
                        .ed-row
                            .ed-col-left  Session Name
                            .ed-col-right Sprout Testing
                        .ed-row
                            .ed-col-left  User ID
                            .ed-col-right 650 865
                        .ed-row
                            .ed-col-left  Meeting ID
                            .ed-col-right 8960
                        .ed-row
                            .ed-col-left  Schedule Date
                            .ed-col-right 2018-09-02  18:16:00
                        .ed-row
                            .ed-col-left  Timezone
